{
    "name": "cron/cron-bundle",
    "type": "symfony-bundle",
    "description": "Symfony cron",
    "license": "MIT",
    "authors": [
        {
            "name": "Dries De Peuter",
            "email": "dries@nousefreak.be"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-pcntl": "*",
        "ext-posix": "*",
        "symfony/framework-bundle": "^5.0|^6.0|^7.0",
        "cron/cron": "^1.0.1",
        "doctrine/doctrine-bundle": "~1.4|~2.0",
        "doctrine/orm": "~2.4,>=2.4.5"
    },
    "autoload": {
        "psr-4" : {
            "Cron\\CronBundle\\" : ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Cron\\CronBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "symfony/security-csrf": "^5.0|^6.0|^7.0",
        "symfony/translation": "^5.0|^6.0|^7.0",
        "symfony/yaml": "^5.0|^6.0|^7.0"
    }
}
